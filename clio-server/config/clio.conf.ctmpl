include "application.conf"

clio.server {
    http-server.port: {{env "CONTAINER_CLIO_PORT"}}

    elasticsearch.http-hosts: [
        {{range $i := loop 3}}
        {
          hostname: "elasticsearch10{{add 1 $i}}-priv.gotc-{{env "ENV"}}.broadinstitute.org",
          port: 9200,
          scheme: http
        },{{end}}
    ]
}
